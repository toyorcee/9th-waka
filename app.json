{
  "expo": {
    "name": "9thwaka",
    "slug": "9thwaka",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Night-Waka.png",
    "scheme": "9thwaka",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/Night-Waka.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/Night-Waka.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Night-Waka.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/Night-Waka.png",
          "color": "#AB8BFF",
          "sounds": []
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you upload profile pictures and vehicle images.",
          "cameraPermission": "The app accesses your camera to take a selfie with your driver license for identity verification."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "9thWaka uses your location to match you with nearby delivery orders when you're online.",
          "locationAlwaysPermission": "9thWaka uses your location to match you with nearby delivery orders when you're online.",
          "locationWhenInUsePermission": "9thWaka uses your location to match you with nearby delivery orders when you're online."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
